{
  "name": "@lifestyle-app/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "dev": "wrangler dev",
    "build": "echo 'Build handled by Wrangler'",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply health-tracker-db",
    "db:migrate:local": "wrangler d1 migrations apply health-tracker-db --local"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.2",
    "@hono/zod-validator": "^0.2.0",
    "@lifestyle-app/shared": "workspace:*",
    "ai": "^6.0.5",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-orm": "^0.30.0",
    "hono": "^4.0.0",
    "nanoid": "^5.1.6",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "drizzle-kit": "^0.21.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.22.0"
  }
}
